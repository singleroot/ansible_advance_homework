[threetierapp:children]
frontends
apps
appdbs
support

[threetierapp:vars]
timeout=60
ansible_become=yes
ansible_user=ec2-user
ansible_ssh_private_key_file="~/.ssh/mykey.pem"
ansible_ssh_common_args="-o StrictHostKeyChecking=no"
